{
    "extends":  "../../tsconfig.json",
    "compilerOptions":  {
                            "outDir":  "dist",
                            "rootDir":  "src",
                            "moduleResolution":  "node",
                            "module":  "esnext",
                            "target":  "es2022",
                            "esModuleInterop":  true,
                            "resolveJsonModule":  true,
                            "types":  [
                                          "node"
                                      ],
                            "skipLibCheck": true
                        },
    "include":  [
                    "src/**/*"
                ]
}

