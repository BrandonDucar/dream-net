{
  "name": "@dreamnet/monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.21.0",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": "24.x",
    "pnpm": ">=10.21.0"
  },
  "pnpm": {
    "overrides": {
      "cloudevents": "npm:@cloudevents/sdk@^3.0.0"
    }
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "server",
    "shared"
  ],
  "scripts": {
    "dev": "pnpm -r --parallel run dev",
    "dev:app": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:admin-dashboard": "pnpm --filter @dreamnet/admin-dashboard dev",
    "build:admin-dashboard": "pnpm --filter @dreamnet/admin-dashboard build",
    "install:ci": "pnpm install --frozen-lockfile",
    "typecheck": "pnpm -r --if-present run typecheck",
    "build": "pnpm -r --if-present run build",
    "build:app": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "lint": "pnpm -r --if-present run lint",
    "format": "pnpm -r --if-present run format",
    "test": "pnpm -r --if-present run test",
    "test:immune-swarm": "tsx scripts/test-immune-swarm.ts",
    "clean": "pnpm -r --if-present run clean",
    "start": "NODE_ENV=production node server/dist/index.js",
    "scan:namecheap": "tsx scripts/scan-namecheap-domains.ts",
    "test:namecheap": "tsx scripts/test-namecheap-api.ts",
    "list:x402": "tsx scripts/list-x402-services.ts",
    "post:social": "tsx scripts/list-x402-and-post.ts",
    "setup:heartbeat": "bash scripts/setup-heartbeat-scheduler.sh",
    "configure:keepalive": "powershell -ExecutionPolicy Bypass -File scripts/configure-cloud-run-keepalive.ps1",
    "check:credits": "tsx scripts/check-cloud-credits.ts",
    "setup:budgets": "tsx scripts/check-cloud-credits.ts",
    "activate:all": "tsx scripts/activate-all-services.ts",
    "check:status": "tsx scripts/check-integration-status.ts",
    "check:api-keeper": "tsx scripts/check-api-keeper-status.ts",
    "check:services": "tsx scripts/check-cloud-run-services.ts",
    "cleanup:services": "powershell -ExecutionPolicy Bypass -File scripts/cleanup-and-deploy-dreamhub.ps1",
    "setup:domains": "powershell -ExecutionPolicy Bypass -File scripts/verify-and-map-domains.ps1",
    "switch:region": "powershell -ExecutionPolicy Bypass -File scripts/switch-to-us-east1.ps1",
    "setup:domains-complete": "powershell -ExecutionPolicy Bypass -File scripts/verify-and-setup-domains-complete.ps1",
    "activate:all": "tsx scripts/activate-all-services.ts",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "gpt5:site": "tsx scripts/gpt5-webgen.ts",
    "compile": "hardhat compile",
    "deploy:base-sepolia": "hardhat run scripts/contracts/deploy.ts --network baseSepolia",
    "deploy:base-mainnet": "hardhat run scripts/contracts/deploy.ts --network baseMainnet",
    "verify:base": "tsx scripts/verifyBase.ts",
    "deploy:base-contracts": "tsx scripts/deploy-base-contracts.ts",
    "deploy:production": "tsx scripts/deploy-production.ts",
    "deploy:gcp": "tsx infrastructure/google/deploy-all.ts",
    "deploy:data-gcp": "tsx infrastructure/google/data/deploy.ts",
    "deploy:gke": "tsx infrastructure/google/gke/deploy.ts",
    "deploy:cloud-run": "gcloud builds submit --config cloudbuild.yaml",
    "deploy:cloud-run:clean": "powershell -ExecutionPolicy Bypass -File scripts/deploy-cloud-run-clean.ps1",
    "deploy:cloud-run:local": "docker build -t dreamnet-local -f Dockerfile . && docker run -p 8080:8080 -e PORT=8080 -e NODE_ENV=production -e INIT_SUBSYSTEMS=false -e INIT_HEAVY_SUBSYSTEMS=false -e MESH_AUTOSTART=false dreamnet-local",
    "deploy:appengine": "gcloud app deploy",
    "deploy:aws": "tsx infrastructure/aws/deploy-all.ts",
    "deploy:data-aws": "tsx infrastructure/aws/data/deploy.ts",
    "deploy:eks": "tsx infrastructure/aws/eks/deploy.ts",
    "vercel-build": "pnpm build:app",
    "build:prebuilt": "npx vercel build --prod --yes",
    "deploy:prebuilt": "npx vercel deploy --prebuilt --prod --yes",
    "vercel:monitor": "tsx scripts/monitor-vercel-build.ts",
    "deploy:vercel-legacy": "tsx scripts/deploy-vercel.ts",
    "enable:gcp-apis": "tsx scripts/enable-all-gcp-apis.ts",
    "test:gcp": "tsx scripts/test-google-cloud-sdk.ts",
    "test:gcp-apis": "tsx scripts/test-all-gcp-apis.ts",
    "test:aws": "tsx scripts/test-aws-sdk.ts",
    "test:clouds": "tsx scripts/test-cloud-sdks.ts",
    "check:gcp-setup": "tsx scripts/check-gcp-setup.ts",
    "check:gcp-accounts": "tsx scripts/check-gcp-accounts.ts",
    "setup:dreamnet-gcp": "tsx scripts/setup-dreamnet-gcp-project.ts",
    "setup:scheduler": "tsx scripts/setup-cloud-scheduler.ts",
    "check:credits": "tsx scripts/check-cloud-credits.ts",
    "setup:internal": "tsx scripts/internal-setup.ts",
    "explore": "tsx scripts/explore-dreamnet.ts",
    "start:server": "powershell -ExecutionPolicy Bypass -File scripts/start-server.ps1",
    "verify:startup": "tsx scripts/verify-startup.ts",
    "verify:docker": "tsx scripts/verify-docker.ts",
    "test:everything": "tsx scripts/test-everything.ts",
    "scan:domains": "tsx scripts/scan-domains.ts",
    "setup:gcp-domains": "tsx scripts/setup-gcp-domains.ts",
    "issue:dreamnet-domains": "tsx scripts/issue-dreamnet-domains.ts",
    "issue:all-verticals": "tsx scripts/issue-all-verticals.ts",
    "verify:connections": "tsx scripts/verify-connections.ts",
    "test:domain-api": "tsx scripts/test-domain-api.ts",
    "deploy:with-dns": "tsx scripts/deploy-with-dns.ts",
    "deploy:dream-domains": "tsx scripts/deploy-dream-domains.ts",
    "deploy:now": "tsx scripts/deploy-now.ts",
    "fix:build-deps": "tsx scripts/fix-build-deps.ts",
    "check:build": "tsx scripts/check-and-fix-build.ts",
    "dreamnet": "tsx scripts/dreamnet-cli.ts",
    "dreamnet:shell": "tsx scripts/dreamnet-shell.ts",
    "monitor:safe-boot": "tsx scripts/monitor-safe-boot.ts",
    "fix:onedrive": "powershell -ExecutionPolicy Bypass -File scripts/fix-onedrive-lock.ps1",
    "force:clean": "powershell -ExecutionPolicy Bypass -File scripts/force-clean-install.ps1",
    "nuclear:clean": "powershell -ExecutionPolicy Bypass -File scripts/nuclear-clean.ps1",
    "move:out-of-onedrive": "powershell -ExecutionPolicy Bypass -File scripts/move-out-of-onedrive.ps1"
  },
