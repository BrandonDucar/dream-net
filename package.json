{
  "name": "@dreamnet/monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.21.0",
  "type": "module",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*",
    "client",
    "server"
  ],
  "scripts": {
    "dev": "pnpm -r --parallel run dev",
    "dev:app": "NODE_ENV=development tsx server/index.ts",
    "install:ci": "pnpm install --frozen-lockfile",
    "typecheck": "pnpm -r --if-present run typecheck",
    "build": "pnpm -r --if-present run build",
    "build:app": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "lint": "pnpm -r --if-present run lint",
    "format": "pnpm -r --if-present run format",
    "test": "pnpm -r --if-present run test",
    "clean": "pnpm -r --if-present run clean",
    "vercel-build": "pnpm build:app",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "gpt5:site": "tsx scripts/gpt5-webgen.ts",
    "build:prebuilt": "npx vercel build --prod --yes",
    "deploy:prebuilt": "npx vercel deploy --prebuilt --prod --yes",
    "compile": "hardhat compile",
    "deploy:base-sepolia": "hardhat run scripts/contracts/deploy.ts --network baseSepolia",
    "deploy:base-mainnet": "hardhat run scripts/contracts/deploy.ts --network baseMainnet",
    "verify:base": "tsx scripts/verifyBase.ts"
  },
  "dependencies": {
