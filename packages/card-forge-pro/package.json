{
    "name": "@dreamnet/card-forge-pro",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "source": "./index.ts",
            "import": "./index.ts",
            "require": "./index.ts",
            "default": "./index.ts"
        },
        "./*": {
            "source": "./src/*.ts",
            "import": "./src/*.ts",
            "default": "./src/*.ts"
        }
    },
    "scripts": {
        "build": "tsc",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "@dreamnet/lib": "workspace:*",
        "@dreamnet/shared": "workspace:*",
        "openai": "^4.0.0"
    },
    "devDependencies": {
        "typescript": "^5.0.0"
    }
}