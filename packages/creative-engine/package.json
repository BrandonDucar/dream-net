{
    "name":  "@dreamnet/creative-engine",
    "version":  "0.1.0",
    "private":  true,
    "type":  "module",
    "main":  "./dist/index.js",
    "types":  "./dist/index.d.ts",
    "exports":  {
                    ".":  {
                              "source":  "./src/index.ts",
                              "import":  "./src/index.ts",
                              "require":  "./src/index.ts",
                              "default":  "./src/index.ts"
                          },
                    "./*":  {
                                "source":  "./src/*.ts",
                                "import":  "./src/*.ts",
                                "default":  "./src/*.ts"
                            },
                    "./src/*":  "./src/*.ts"
                },
    "scripts":  {
                    "build":  "tsc",
                    "typecheck":  "tsc --noEmit",
                    "clean":  "rm -rf dist",
                    "dev":  "tsc -w"
                },
    "dependencies":  {
                         "@dreamnet/shared":  "workspace:*",
                         "@dreamnet/lib":  "workspace:*"
                     },
    "devDependencies":  {
                            "typescript":  "^5.0.0"
                        }
}
