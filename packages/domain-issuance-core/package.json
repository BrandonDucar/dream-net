{
    "name":  "@dreamnet/domain-issuance-core",
    "version":  "1.0.0",
    "private":  true,
    "type":  "module",
    "main":  "dist/index.js",
    "types":  "dist/index.d.ts",
    "exports":  {
                    ".":  {
                              "source":  "./src/index.ts",
                              "import":  "./src/index.ts",
                              "require":  "./src/index.ts",
                              "default":  "./src/index.ts"
                          },
                    "./*":  {
                                "source":  "./src/*.ts",
                                "import":  "./src/*.ts",
                                "default":  "./src/*.ts"
                            },
                    "./src/*":  "./src/*.ts"
                },
    "scripts":  {
                    "dev":  "tsx watch src/index.ts",
                    "build":  "tsc",
                    "typecheck":  "tsc --noEmit"
                },
    "dependencies":  {
                         "@dreamnet/lib":  "workspace:*",
                         "@dreamnet/shared":  "workspace:*",
                         "nanoid":  "^5.0.7"
                     },
    "devDependencies":  {
                            "tsx":  "^4.20.6",
                            "typescript":  "^5.4.5"
                        }
}
