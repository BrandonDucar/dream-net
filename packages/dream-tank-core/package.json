{
    "name":  "@dreamnet/dream-tank-core",
    "version":  "1.0.0",
    "type":  "module",
    "main":  "./dist/index.js",
    "types":  "./dist/index.d.ts",
    "exports":  {
                    ".":  {
                              "source":  "./index.ts",
                              "import":  "./index.ts",
                              "require":  "./index.ts",
                              "default":  "./index.ts"
                          },
                    "./*":  {
                                "source":  "./*.ts",
                                "import":  "./*.ts",
                                "default":  "./*.ts"
                            }
                },
    "scripts":  {
                    "build":  "tsc",
                    "typecheck":  "tsc --noEmit",
                    "clean":  "rm -rf dist"
                },
    "private":  true,
    "dependencies":  {
                         "@dreamnet/dream-cortex":  "workspace:*",
                         "@dreamnet/dream-vault":  "workspace:*",
                         "@dreamnet/field-layer":  "workspace:*",
                         "@dreamnet/identity-grid":  "workspace:*",
                         "@dreamnet/lib":  "workspace:*",
                         "@dreamnet/narrative-field":  "workspace:*",
                         "@dreamnet/neural-mesh":  "workspace:*",
                         "@dreamnet/reputation-lattice":  "workspace:*",
                         "@dreamnet/shared":  "workspace:*",
                         "@dreamnet/utils":  "workspace:*"
                     },
    "devDependencies":  {
                            "typescript":  "^5.0.0"
                        }
}
