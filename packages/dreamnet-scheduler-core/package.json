{
    "name":  "@dreamnet/dreamnet-scheduler-core",
    "version":  "0.1.0",
    "private":  true,
    "type":  "module",
    "main":  "./dist/index.js",
    "types":  "./dist/index.d.ts",
    "exports":  {
                    ".":  {
                              "source":  "./index.ts",
                              "import":  "./index.ts",
                              "require":  "./index.ts",
                              "default":  "./index.ts"
                          },
                    "./*":  {
                                "source":  "./*.ts",
                                "import":  "./*.ts",
                                "default":  "./*.ts"
                            }
                },
    "scripts":  {
                    "build":  "tsc",
                    "typecheck":  "tsc --noEmit",
                    "clean":  "rm -rf dist"
                },
    "dependencies":  {
                         "@dreamnet/lib":  "workspace:*",
                         "@dreamnet/shared":  "workspace:*",
                         "@dreamnet/dreamnet-control-core":  "workspace:*",
                         "@dreamnet/dreamnet-audit-core":  "workspace:*",
                         "@dreamnet/dreamnet-operational-bridge":  "workspace:*",
                         "@dreamnet/dreamnet-os-core":  "workspace:*"
                     },
    "devDependencies":  {
                            "typescript":  "^5.9.3"
                        }
}
