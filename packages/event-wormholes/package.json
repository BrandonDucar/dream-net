{
    "name": "@dreamnet/event-wormholes",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "source": "./index.ts",
            "import": "./index.ts",
            "require": "./index.ts",
            "default": "./index.ts"
        },
        "./*": {
            "source": "./*.ts",
            "import": "./*.ts",
            "default": "./*.ts"
        }
    },
    "scripts": {
        "build": "tsc",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "@dreamnet/internal-ports": "workspace:*",
        "@dreamnet/internal-router": "workspace:*",
        "@dreamnet/lib": "workspace:*",
        "@dreamnet/shared": "workspace:*"
    },
    "devDependencies": {
        "typescript": "^5.0.0"
    }
}