{
    "name": "@dreamnet/expansion-core",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "source": "./src/index.ts",
            "import": "./src/index.ts",
            "require": "./src/index.ts",
            "default": "./src/index.ts"
        },
        "./*": {
            "source": "./src/*.ts",
            "import": "./src/*.ts",
            "default": "./src/*.ts"
        },
        "./src/*": "./src/*.ts"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc -w",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "@dreamnet/dreamnet-os-core": "workspace:*",
        "@dreamnet/shared": "workspace:*",
        "node-fetch": "^3.3.2"
    },
    "devDependencies": {
        "typescript": "^5.3.3"
    }
}
