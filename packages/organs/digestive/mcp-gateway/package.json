{
    "name":  "@dreamnet/mcp-gateway",
    "version":  "0.1.0",
    "private":  true,
    "type":  "module",
    "main":  "./dist/index.js",
    "types":  "./dist/index.d.ts",
    "exports":  {
                    ".":  {
                              "source":  "./src/index.ts",
                              "import":  "./src/index.ts",
                              "require":  "./src/index.ts",
                              "default":  "./src/index.ts"
                          },
                    "./*":  {
                                "source":  "./src/*.ts",
                                "import":  "./src/*.ts",
                                "default":  "./src/*.ts"
                            },
                    "./src/*":  "./src/*.ts"
                },
    "scripts":  {
                    "build":  "tsc",
                    "typecheck":  "tsc --noEmit",
                    "clean":  "rm -rf dist"
                },
    "dependencies":  {
                         "@dreamnet/agent-gateway":  "workspace:*",
                         "@modelcontextprotocol/sdk":  "^1.6.0"
                     },
    "devDependencies":  {
                            "typescript":  "^5.9.3",
                            "@types/node":  "^22.7.5"
                        }
}
