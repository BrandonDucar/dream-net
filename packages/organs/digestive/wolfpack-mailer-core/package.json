{
    "name":  "@dreamnet/wolfpack-mailer-core",
    "version":  "0.1.0",
    "private":  true,
    "type":  "module",
    "main":  "./dist/index.js",
    "types":  "./dist/index.d.ts",
    "exports":  {
                    ".":  {
                              "source":  "./index.ts",
                              "import":  "./index.ts",
                              "require":  "./index.ts",
                              "default":  "./index.ts"
                          },
                    "./*":  {
                                "source":  "./*.ts",
                                "import":  "./*.ts",
                                "default":  "./*.ts"
                            }
                },
    "scripts":  {
                    "typecheck":  "tsc --noEmit"
                },
    "dependencies":  {
                         "@dreamnet/lib":  "workspace:*",
                         "@dreamnet/shared":  "workspace:*",
                         "nodemailer":  "^6.9.16"
                     },
    "devDependencies":  {
                            "@types/nodemailer":  "^6.4.15",
                            "typescript":  "^5.9.3"
                        }
}
