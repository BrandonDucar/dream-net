{
    "name":  "@dreamnet/organ-lung",
    "version":  "1.0.0",
    "private":  true,
    "type":  "module",
    "main":  "dist/index.js",
    "types":  "dist/index.d.ts",
    "exports":  {
                    ".":  {
                              "source":  "./src/index.ts",
                              "import":  "./src/index.ts",
                              "require":  "./src/index.ts",
                              "default":  "./src/index.ts"
                          },
                    "./*":  {
                                "source":  "./src/*.ts",
                                "import":  "./src/*.ts",
                                "default":  "./src/*.ts"
                            },
                    "./src/*":  "./src/*.ts"
                },
    "scripts":  {
                    "build":  "tsc",
                    "typecheck":  "tsc --noEmit"
                },
    "dependencies":  {
                         "@dreamnet/lib":  "workspace:*",
                         "@dreamnet/nerve":  "workspace:*",
                         "@dreamnet/platform-connector":  "workspace:*",
                         "zod":  "^3.24.2"
                     },
    "devDependencies":  {
                            "typescript":  "^5.9.3"
                        }
}
