{
    "name":  "@dreamnet/agent-wallet-manager",
    "version":  "1.0.0",
    "description":  "Wallet management system for AI agents",
    "main":  "./dist/index.js",
    "types":  "./dist/index.d.ts",
    "exports":  {
                    ".":  {
                              "source":  "./index.ts",
                              "import":  "./index.ts",
                              "require":  "./index.ts",
                              "default":  "./index.ts"
                          },
                    "./*":  {
                                "source":  "./src/*.ts",
                                "import":  "./src/*.ts",
                                "default":  "./src/*.ts"
                            },
                    "./src/*":  "./src/*.ts"
                },
    "scripts":  {
                    "build":  "tsc",
                    "typecheck":  "tsc --noEmit",
                    "clean":  "rm -rf dist"
                },
    "dependencies":  {
                         "@dreamnet/lib":  "workspace:*",
                         "@dreamnet/shared":  "workspace:*",
                         "ethers":  "^6.15.0"
                     },
    "devDependencies":  {
                            "typescript":  "^5.9.3"
                        },
    "type":  "module"
}
