{
    "name":  "@dreamnet/antigravity-core",
    "version":  "1.0.0",
    "type":  "module",
    "main":  "./dist/index.js",
    "types":  "./dist/index.d.ts",
    "exports":  {
                    ".":  {
                              "source":  "./index.ts",
                              "import":  "./index.ts",
                              "require":  "./index.ts",
                              "default":  "./index.ts"
                          },
                    "./*":  {
                                "source":  "./src/*.ts",
                                "import":  "./src/*.ts",
                                "default":  "./src/*.ts"
                            },
                    "./src/*":  "./src/*.ts"
                },
    "dependencies":  {
                         "@dreamnet/memory-dna":  "workspace:*",
                         "@dreamnet/nerve":  "workspace:*",
                         "@dreamnet/spore-engine":  "workspace:*",
                         "@dreamnet/shared":  "workspace:*",
                         "langchain":  "0.0.186"
                     },
    "scripts":  {
                    "build":  "tsc",
                    "typecheck":  "tsc --noEmit",
                    "clean":  "rm -rf dist"
                }
}
