{
    "name": "@dreamnet/sensory-spikes",
    "version": "1.0.0",
    "description": "Real-world data ingestion spikes for DreamNet (Metals, Crypto, Science, News)",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "source": "./src/index.ts",
            "import": "./src/index.ts",
            "default": "./src/index.ts"
        },
        "./spikes/*": "./src/spikes/*.ts"
    },
    "scripts": {
        "build": "tsc",
        "dev": "tsc -w",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "axios": "^1.13.2",
        "ws": "^8.18.3"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/ws": "^8.5.0",
        "typescript": "^5.0.0"
    }
}