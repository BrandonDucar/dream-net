{
  "name": "RedpandaToCloudflare",
  "source": "Redpanda",
  "target": "CloudflareQueues",
  "purpose": "Forward analytics insights to edge workers",
  "config": {
    "sourceTopics": [
      "HUMAN_RESONANCE_EVENTS",
      "TEMPORAL_ECONOMICS"
    ],
    "targetQueues": [
      "EDGE_TASKS",
      "VERIFICATION_REQUESTS"
    ],
    "batchSize": 50,
    "flushInterval": 10000
  }
}