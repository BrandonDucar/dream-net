{
    "name": "@dreamnet/spore-engine",
    "version": "1.0.0",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "source": "./src/index.ts",
            "import": "./src/index.ts",
            "require": "./src/index.ts",
            "default": "./src/index.ts"
        },
        "./*": {
            "source": "./src/*.ts",
            "import": "./src/*.ts",
            "default": "./src/*.ts"
        }
    },
    "scripts": {
        "build": "tsc",
        "typecheck": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "private": true,
    "dependencies": {
        "@dreamnet/lib": "workspace:*",
        "@dreamnet/shared": "workspace:*",
        "@dreamnet/utils": "workspace:*",
        "@dreamnet/event-wormholes": "workspace:*",
        "@dreamnet/graft-engine": "workspace:*",
        "@dreamnet/internal-ports": "workspace:*"
    },
    "devDependencies": {
        "typescript": "^5.0.0"
    }
}