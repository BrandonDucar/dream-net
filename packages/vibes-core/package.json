{
    "name":  "@dreamnet/vibes-core",
    "version":  "0.1.0",
    "private":  true,
    "type":  "module",
    "main":  "./dist/index.js",
    "types":  "./dist/index.d.ts",
    "exports":  {
                    ".":  {
                              "source":  "./index.ts",
                              "import":  "./index.ts",
                              "require":  "./index.ts",
                              "default":  "./index.ts"
                          },
                    "./*":  {
                                "source":  "./*.ts",
                                "import":  "./*.ts",
                                "default":  "./*.ts"
                            }
                },
    "scripts":  {
                    "build":  "tsc",
                    "dev":  "tsc -w"
                },
    "devDependencies":  {
                            "typescript":  "^5.0.0"
                        }
}
