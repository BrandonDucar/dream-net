{
  "name": "@dreamnet/server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx index.ts",
    "build": "node build.cjs",
    "build:full": "cd .. && pnpm --filter client build && node build.cjs",
    "start": "node dist/index.js",
    "start:dev": "tsx index.ts",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\""
  },
  "dependencies": {
    "@dreamnet/deployment-core": "workspace:*",
    "@dreamnet/directory": "workspace:*",
    "@dreamnet/dreamnet-control-core": "workspace:*",
    "@dreamnet/internal-ports": "workspace:*",
    "@dreamnet/metrics-engine": "workspace:*",
    "@dreamnet/nerve": "workspace:*",
    "@dreamnet/network-blueprints": "workspace:*",
    "@dreamnet/ops-sentinel": "workspace:*",
    "axios": "^1.13.2",
    "blake3": "^2.1.7",
    "googleapis": "^128.0.0",
    "helmet": "^8.1.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.7",
    "node-cron": "^3.0.3",
    "snarkjs": "^0.7.5",
    "stripe": "^14.21.0",
    "vm2": "^3.10.0"
  },
  "devDependencies": {
    "@types/multer": "^1.4.12",
    "@vitest/ui": "^2.0.0",
    "supertest": "^7.0.0",
    "tsx": "^4.20.6",
    "vitest": "^2.0.0"
  }
}
