    "@dreamnet/nerve": "workspace:*",
    "blake3": "^2.1.7",
    "nanoid": "^5.0.7",
    "node-cron": "^3.0.3",
    "snarkjs": "^0.7.5",
    "multer": "^1.4.5-lts.1",
    "stripe": "^14.21.0"
  },
  "devDependencies": {
    "tsx": "^4.20.6",
    "@types/multer": "^1.4.12"
  }
