{
  "compilerOptions": {
    "target": "ES2022",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": [
      "node"
    ],
    "noErrorTruncation": true,
    "baseUrl": ".",
    "paths": {
      "@dreamnet/spine": [
        "spine/index.ts"
      ],
      "@dreamnet/*": [
        "packages/organs/integumentary/*/src/index.ts",
        "packages/organs/integumentary/*/index.ts",
        "packages/organs/immune/*/src/index.ts",
        "packages/organs/lymphatic/*/src/index.ts",
        "packages/organs/skeletal/*/src/index.ts",
        "packages/organs/reproductive/*/src/index.ts",
        "packages/organs/nervous/*/src/index.ts",
        "packages/organs/nervous/*/index.ts",
        "packages/organs/respiratory/*/src/index.ts",
        "packages/organs/respiratory/*/index.ts",
        "packages/organs/circulatory/*/src/index.ts",
        "packages/organs/circulatory/*/index.ts",
        "packages/organs/digestive/*/src/index.ts",
        "packages/organs/digestive/*/index.ts",
        "packages/organs/muscular/*/src/index.ts",
        "packages/organs/muscular/*/index.ts",
        "packages/organs/excretory/*/src/index.ts",
        "packages/organs/excretory/*/index.ts",
        "packages/organs/endocrine/*/src/index.ts",
        "packages/organs/endocrine/*/index.ts",
        "packages/*"
      ],
      "@/*": [
        "packages/organs/integumentary/client/src/*"
      ]
    }
  }
}