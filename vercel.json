{
  "version": 2,
  "rootDirectory": "client",
  "buildCommand": "pnpm run build",
  "installCommand": "cd .. && pnpm --filter client... install --no-frozen-lockfile",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://api.dreamnet.ink/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
