{
  "version": 2,
  "rootDirectory": "client",
  "installCommand": "corepack enable pnpm && pnpm install --no-frozen-lockfile",
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://api.dreamnet.ink/api/$1"
    }
  ]
}
