{
    "version": 2,
    "buildCommand": "cd apps/portal && pnpm run build",
    "outputDirectory": "apps/portal/dist_production",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://dreamnet.ink/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "installCommand": "pnpm install --no-frozen-lockfile",
    "framework": "vite"
}